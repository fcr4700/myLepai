<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="qf.com.myprojectlepai">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WebViewActivity"/>
        <activity android:name=".myActivity.LogActivity">
            <intent-filter>
                <action android:name="com.activity_logActivity"/>
            </intent-filter>
        </activity>
        <activity android:name=".myActivity.ShareActivity">
            <intent-filter>
                <action android:name="com.activity_shareAcitivity"/>
            </intent-filter>
        </activity>
        <activity android:name=".myActivity.AboutUsActivity"/>
        <activity android:name=".myActivity.RegisterActivity"/>
        <activity android:name=".myActivity.BbsRT"/>
        <activity android:name=".myActivity.PicShowActivity"/>
        <activity android:name=".myActivity.Welcome"/>
    </application>

</manifest>