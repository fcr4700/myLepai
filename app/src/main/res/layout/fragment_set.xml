<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginLeft="15dp"
        android:background="@drawable/bbs_item_sub_class_bg_normal"
        android:layout_marginTop="1dp"
        android:orientation="horizontal">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/set_ralatveZ1Id">
        <ImageView
            android:layout_width="27dp"
            android:layout_height="match_parent"
            android:id="@+id/set_imgZ1Id"
            android:src="@drawable/set_icon_account"/>
        <TextView
            android:layout_width="200dp"
            android:layout_height="60dp"
            android:text="注册/登录蜂鸟"
            android:layout_toRightOf="@id/set_imgZ1Id"
            android:gravity="center_vertical"
            android:textSize="16sp"/>

        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginBottom="10dp"
        android:layout_marginLeft="15dp"
        android:background="@drawable/bbs_item_sub_class_bg_normal"
        android:orientation="horizontal"
        android:layout_marginTop="1dp">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/set_ralatveZ2Id">
            <ImageView
                android:layout_width="27dp"
                android:layout_height="match_parent"
                android:id="@+id/set_imgZ2Id"
                android:src="@drawable/bbs_icon_5"/>
            <TextView
                android:layout_width="200dp"
                android:layout_height="60dp"
                android:text="分享账号管理"
                android:layout_toRightOf="@id/set_imgZ2Id"
                android:gravity="center_vertical"
                android:textSize="16sp"/>

        </RelativeLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginLeft="15dp"
        android:orientation="horizontal"
        android:background="@drawable/bbs_item_sub_class_bg_normal"
        android:layout_marginTop="1dp">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/set_ralatveZ3Id">
            <ImageView
                android:layout_width="27dp"
                android:layout_height="match_parent"
                android:id="@+id/set_imgZ3Id"
                android:src="@drawable/bbs_icon_1"/>
            <TextView
                android:layout_width="200dp"
                android:layout_height="60dp"
                android:text="我的论坛"
                android:layout_toRightOf="@id/set_imgZ3Id"
                android:gravity="center_vertical"
                android:textSize="16sp"/>



        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:orientation="horizontal"
        android:layout_marginLeft="15dp"
        android:background="@drawable/bbs_item_sub_class_bg_normal"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="1dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/set_ralatveZ4Id">
            <ImageView
                android:layout_width="27dp"
                android:layout_height="match_parent"
                android:id="@+id/set_imgZ4Id"
                android:src="@drawable/set_icon_favorite"/>
            <TextView
                android:layout_width="200dp"
                android:layout_height="60dp"
                android:text="个人收藏"
                android:layout_toRightOf="@id/set_imgZ4Id"
                android:gravity="center_vertical"
                android:textSize="16sp"/>

        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:orientation="horizontal"
        android:layout_marginLeft="15dp"
        android:background="@drawable/bbs_item_sub_class_bg_normal"
        android:layout_marginTop="1dp">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/set_ralatveZ5Id">
            <ImageView
                android:layout_width="27dp"
                android:layout_height="match_parent"
                android:id="@+id/set_imgZ5Id"
                android:src="@drawable/set_icon_clear_cache"/>
            <TextView
                android:layout_width="200dp"
                android:layout_height="60dp"
                android:text="清除应用缓存"
                android:layout_toRightOf="@id/set_imgZ5Id"
                android:gravity="center_vertical"
                android:textSize="16sp"/>

        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginLeft="15dp"
        android:orientation="horizontal"
        android:background="@drawable/bbs_item_sub_class_bg_normal"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="1dp">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/set_ralatveZ6Id">
            <ImageView
                android:layout_width="27dp"
                android:layout_height="match_parent"
                android:id="@+id/set_imgZ6Id"
                android:src="@drawable/set_icon_auto_image"/>
            <TextView
                android:layout_width="200dp"
                android:layout_height="60dp"
                android:text="自动加载图片"
                android:layout_toRightOf="@id/set_imgZ6Id"
                android:gravity="center_vertical"
                android:textSize="16sp"/>


            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginTop="15dp"
                android:background="@drawable/set_switch"/>

        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginLeft="15dp"
        android:orientation="horizontal"
        android:background="@drawable/bbs_item_sub_class_bg_normal"
        android:layout_marginTop="1dp">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/set_ralatveZ7Id">
            <ImageView
                android:layout_width="27dp"
                android:layout_height="match_parent"
                android:id="@+id/set_imgZ7Id"
                android:src="@drawable/bbs_icon_0"/>
            <TextView
                android:layout_width="200dp"
                android:layout_height="60dp"
                android:text="建议反馈"
                android:layout_toRightOf="@id/set_imgZ7Id"
                android:gravity="center_vertical"
                android:textSize="16sp"/>

        </RelativeLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginLeft="15dp"
        android:orientation="horizontal"
        android:background="@drawable/bbs_item_sub_class_bg_normal"
        android:layout_marginTop="1dp">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/set_ralatveZ8Id">
            <ImageView
                android:layout_width="27dp"
                android:layout_height="match_parent"
                android:id="@+id/set_imgZ8Id"
                android:src="@drawable/bbs_icon_6"/>
            <TextView
                android:layout_width="200dp"
                android:layout_height="60dp"
                android:text="关于我们"
                android:layout_toRightOf="@id/set_imgZ8Id"
                android:gravity="center_vertical"
                android:textSize="16sp"/>
        </RelativeLayout>
    </LinearLayout>
</LinearLayout>
